configuration {
	display-emoji: "ðŸ’€xðŸ©·";
	show-icons: false;
	me-select-entry: "";
	me-accept-entry: "MousePrimary";
}

* {
	font: "Noto Sans 11";
	text-color: #eff0eb;
	background-color: #282738FF;
	border-color: #00000000;
	width: 684px;
	height: 477px;
	location: 0;
}

window {
	border-radius: 16px;
}

inputbar {
	border: 16px;
	children: [prompt,entry];
}

prompt {
	font: "Noto Sans 18";
	vertical-align: 0.5;
	padding: 0px 16px 0px 16px;
	border: 0px;
}

entry {
	font: "Noto Sans 14";
	padding: 8px 16px 0 0;
	border: 0px;
}

textbox {
	background-color: #282738FF;
	border-color: #282738FF;
	border: 0px;
	padding: 8px 12px;
}

listview {
	cycle: false;
	scrollbar: false;
	columns: 9;
	cycle: false;
	dynamic: false;
	layout: vertical;
	padding: 8px 10px 10px 10px;
}

element {
	border-radius: 3px;
	padding: 14px 0px 14px 0px;
}

element-text {
	font: "Noto Color Emoji 27";
	expand: false;
	text-color: #FFFFFFFF;
	horizontal-align: 1;
	vertical-align: 0.5;
	margin: 0px 0px 0px 13px;
	width: 1px;
	padding: 1px 0px 16px;
	background-color: #FFFFFF00;
}

element selected {
	background-color: #E5B9C6FF;
}